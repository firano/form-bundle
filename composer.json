{
    "name":         "firano/form-bundle",
    "description":  "FORK! Fix for symfony 2.3. This bundle provides extra form types empowered by jQuery",
    "keywords":     ["form", "jqueryui", "jQuery", "Symfony", "plupload", "tinymce", "inputlimiter", "select2", "recaptcha"],
    "type":         "symfony-bundle",
    "license":      "MIT",
    "authors":      [
        {
            "name":  "Nikolay Georgiev",
            "email": "azazen09@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.*-dev"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "symfony/framework-bundle": "2.3.*",
        "firano/datagrid-bundle": "dev-master",
        "avalanche123/imagine-bundle": "v2.1"
    },
    "require-dev": {
        "neutron/component-bundle": "*"
    },
    "repositories":[
        {
            "type":"git",
            "url":"http://github.com/firano/datagrid-bundle"
        }
    ],
    "autoload": {
        "psr-0": { "Neutron\\FormBundle": "" }
    },
    "target-dir": "Neutron/FormBundle"
}
